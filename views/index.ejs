<!doctype html>
<html lang="en">
<head>
<meta charset="UTF-8">
<title>Single page web app using Angularjs</title>
<link href="././bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css">
</head>
<body>
  <div class="container">
      <div class="jumbotron text-center">
        <h1 class="display-3">Form update</h1>
        <p class="lead">Jumbo helper text</p>
        <hr class="my-2">
        
      </div>
    <!-- <div class="row"> -->

      <form action="" method="post" enctype="multipart/form-data">

          <div class="form-group">
              <label for="" class="">Display value : </label>
              <input type="text"
              name="tdsp" id="" class="form-control" aria-describedby="helpId" placeholder="Title product!!!">
              <small class="form-text text-muted" id="helpId">Help Text</small>
          </div>
          <div class="form-group">
            <label for="image" class="">Image produce : </label>
            <input type="file"
            name="anhsp" id="" class="form-control-file" aria-describedby="helpId">
            <small class="form-text text-muted" id="helpId">Help Text</small>
        </div>

          <button type="submit" class="btn btn-primary btn-block">Send information</button>

      </form>

      <!-- // Texxt in th line -->
      
    <!-- </div> -->
    
  </div>
 
<script src="././bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<script src="././bootstrap/js/bootstrap.min.js.map" type="text/javascript"></script>

</body>
</html>